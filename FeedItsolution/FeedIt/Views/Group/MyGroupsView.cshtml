@model List<FeedIt.Models>

@{
    ViewBag.Title = "My groups";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<body>
    <h2 style="text-align:center"><span>My </span><span class="newsfeed">Groups</span></h2>
    <div class="wrapper">
        <div class="navleft">
        </div>
        <div class="navcentermygroups">
            @foreach (var item in Model.myGroups)
            {
                if (item != null)
                {
                    <div class="mygroups">
                        <div>
                            <a href="@Url.Action("GroupView", "Group", new { id = item.ID})">
                                <img src="@Url.Content(item.picture)" alt="picture" class="mygroupspicture" />
                            </a>
                            <a href=@Url.Action("GroupView", "Group", new { id = item.ID }) class="mygroupsname">@item.name</a>
                        </div>
                    </div>
                }
            }
        </div>
        <div class="navright">
        </div>
    </div>
</body>