@model FeedIt.Models.GroupList

@{
    ViewBag.Title = "Group Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>
    <h2>My groups</h2>
    <div class="wrapper">
        <div class="navleft">
        <p>My groups<p>
    </div>
        <div class="navcentermygropus">
            @foreach (var item in Model.myGroups)
            {
                <div class="mygroups">
                    <div>
                        <a href="@Url.Action("GroupView", "Group", new { id = item.ID})">
                            <img src="@Url.Content(item.picture)" alt="picture" class="groupprofilepicture" />
                        </a>
                    </div>
                    <div>
                        <a href=@Url.Action("GroupView", "Group", new { id = item.ID })>@item.name</a>
                    </div>
                </div>
            }
        </div>
        <div class="navright">
            <h3>Einar</h3>
        </div>
    </div>
</body>